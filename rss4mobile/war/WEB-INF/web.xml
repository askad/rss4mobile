<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>TestInitData</servlet-name>
		<servlet-class>yy.test.TestService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestInitData</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FetchSrcArticle</servlet-name>
		<servlet-class>yy.servlet.FetchSrcArticleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FetchSrcArticle</servlet-name>
		<url-pattern>/srcArticle</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Control</servlet-name>
		<servlet-class>yy.servlet.ControlServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Control</servlet-name>
		<url-pattern>/ctrl</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
